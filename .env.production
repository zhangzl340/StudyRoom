# 生产环境变量
# 环境标识
NODE_ENV=production

# 应用配置
VITE_APP_TITLE=高校自习室智能预约平台
VITE_APP_VERSION=1.0.0

# API配置（需要根据实际部署地址修改）
VITE_API_BASE_URL=/api
VITE_WS_BASE_URL=/ws

# 功能开关
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEBUG=false

# 超时配置
VITE_REQUEST_TIMEOUT=15000
VITE_WS_RECONNECT_INTERVAL=5000

# 其他
VITE_PAGE_SIZE=20
VITE_MAX_RESERVATION_HOURS=4